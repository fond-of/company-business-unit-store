<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CompanyBusinessUnitStore">
        <property name="idCompanyBusinessUnitStore" type="int"/>
        <property name="name" type="string"/>
    </transfer>

    <transfer name="CompanyBusinessUnitStoreResponse">
        <property name="hasCompanyBusinessUnitStore" type="bool" />
        <property name="companyBusinessUnitStoreTransfer" type="CompanyBusinessUnitStore" />
        <property name="isSuccess" type="bool" />
        <property name="errors" type="CompanyBusinessUnitStoreError[]" singular="error" />
    </transfer>

    <transfer name="CompanyBusinessUnitStoreError">
        <property name="message" type="string" />
    </transfer>

    <transfer name="CompanyBusinessUnitStoreCollection">
        <property name="filter" type="Filter" />
        <property name="companyBusinessUnitStores" type="CompanyBusinessUnitStore[]" singular="companyBusinessUnitStore" />
        <property name="pagination" type="Pagination" />
    </transfer>

